{
  "expo": {
    "name": "Links Hub",
    "slug": "links_hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "links_hub",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.myyourtheir.links_hub"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/link.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-share-intent",
        {
          "androidIntentFilters": [
            "*/*"
          ]
        }
      ],
      "react-native-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e301bf0c-a9eb-4e44-b698-23abcd77e804"
      }
    }
  }
}
